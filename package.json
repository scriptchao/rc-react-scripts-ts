{
  "name": "rc-react-scripts-ts",
  "version": "2.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lib": "rimraf lib && npm run lib:types && npm run lib:js",
    "lib:js": "babel src/Main --out-dir lib --extensions .ts,.tsx --source-maps inline --copy-files",
    "lib:types": "tsc -p ./tsconfig.declaration.json",
    "analyzer": "node scripts/build.js --analyzer=true",
    "serve": "serve -s build",
    "test": "node scripts/test.js --watch",
    "test-debug": "node --inspect-brk scripts/test.js --runInBand",
    "coverage": "node scripts/test.js --coverage",
    "codecov": "npm run coverage && codecov",
    "lint": "eslint src --max-warnings 0 --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "antd": "^3.26.3",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-destructuring": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "10.x",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "codecov": "^3.6.5",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.2",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^10.0.0",
    "rimraf": "^3.0.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.1",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "yargs": "^15.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 8"
  ]
